<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title></title>
  </head>
  <body>
    <table border="1">
      <thead>
        <tr>
          <th>ID</th>
          <th>Username</th>
          <th>Password</th>
          <th>Secret</th>
          <th>Role</th>
          <th>ID Karyawan</th>
          <th>Nama Karyawan</th>
        </tr>
      </thead>
      <tbody>
        <% dataUsers.forEach(user => { %>
          <tr>
            <td><%= user.id %></td>
            <td><%= user.username %></td>
            <td><%= user.password %></td>
            <td><%= user.secret %></td>
            <td><%= user.role %></td>
            <td><%= user.Karyawan.id %></td>
            <td><%= user.Karyawan.nama %></td>
            <td><a href="/user/delete/<%= user.id %>">DELETE</a></td>
            <td><a href="/user/edit/<%= user.id %>">EDIT</a></td>
          </tr>
        <% }) %>
      </tbody>
    </table>
  </body>
</html>
